<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomBarItem">
        <attr name="icon_normal" format="reference"/>
        <attr name="icon_selected" format="reference"/>
        <attr name="item_text" format="string"/>
        <attr name="item_text_size" format="dimension"/>
        <attr name="item_text_color_normal" format="color"/>
        <attr name="item_text_color_selected" format="color"/>
        <!-- 图标和文字标题之间的距离 -->
        <attr name="item_margin_top" format="dimension"/>

        <!--是否开启触摸背景效果-->
        <attr name="open_touch_bg" format="boolean"/>
        <!--设置触摸背景-->
        <attr name="touch_drawable" format="reference"/>
        
    </declare-styleable>
</resources>